<br/>
<ul class="nav nav-tabs">
  <li ng-class="{active: activeMenu == 'me'}">
    <a ui-sref="main.results">Your Results</a>
  </li>
  <li ng-class="{active: activeMenu == 'all'}">
    <a ui-sref="main.results.all">Everyone's Results</a>
  </li>
  <li ng-class="{active: activeMenu == 'personality'}">
    <a ui-sref="main.results.personality">Your Personality</a>
  </li>
</ul>
<div class="results-container ui-view">
  <div class="panel-header">
    <div class="panel panel-default panel-half">
      <div class="panel-heading">
        <h3 class="panel-title">Overall Record (Percentage)</h3>
      </div>
      <div class="panel-body">
        <h1 ng-show="myStats.correct_answers">
          {{myStats.correct_answers}}/{{myStats.total_answers}}
          ({{(myStats.correct_answers/myStats.total_answers * 100).toFixed(2)}}%)
        </h1>
      </div>
    </div>
  
    <div class="panel panel-default panel-half">
      <div class="panel-heading">
        <h3 class="panel-title">Number of Likes</h3>
      </div>
      <div class="panel-body" ng-show="myStats.likes">
        <h1>{{myStats.likes}} Pages</h1>
      </div>
    </div>
  
    <div style="clear: both"></div>
  </div>

  <div class="panel panel-default">
    <div class="panel-heading">
      <h3 class="panel-title">Results Over Time</h3>
    </div>
    <div class="panel-body">
      <div ng-controller="MyResultsCtrl">
        <highchart config="chartConfig"></highchart>
      </div>
    </div>
  </div>
</div>